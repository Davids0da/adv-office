      <img style="height: 100%; object-fit:cover;" src="images/demo/backgrounds/reference.jpg" alt="aboutus">
    </div>

    <div class="center" align="center">
      <img style="width: 60px; margin-top: 50px; margin-bottom: 10px;" src="/images/vaga-logo.png" alt="copyright-logo">
      <h2 style="width: 181px; margin: auto;" id="contact-title">
        <%= __('referencetitle') %>
      </h2>
    </div>

    <div class="wrapper container">
      <div class="hoc clear">
        <p class="field-about justify">
          <%= __('reference1') %>
        </p>
        <h3 align="center" class="field-about">
          <%= __('reference2') %>
        </h3>
        <p class="field-about justify">
          <%= __('reference3') %>
        </p>
