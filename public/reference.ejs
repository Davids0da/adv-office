  <body>
    <p id="border-t"></p>
    <div class="wrapper row0">
      <div id="topbar" class="hoc clear">
        <div class="fl_left">
          <ul class="nospace">
            <li>
              <i style="color: #D3B889" class="fa fa-envelope-o"></i class="sm-letters"> info@lawofficebelgrade.com</li>
            <li>
              <i style="color: #D3B889" class="fa fa-phone"></i> +00 (381) 24 32 800</li>
          </ul>
        </div>
        <div class="fl_right">
            <a href="#" style="color: #D3B889; padding-right: 7px;"> Choose language: </a>
            <a href="/en">
            <img style="width: 25px;" src="images/demo/backgrounds/en.png">
            </a>
            <a id="francuski" href="/fr">
              <img style="width: 25px; margin-left: 5px;margin-right: 5px;" src="images/demo/backgrounds/fr.png">
            </a>
            <a href="/srb">
              <img style="width: 25px;" src="images/demo/backgrounds/srb.png">
            </a>
