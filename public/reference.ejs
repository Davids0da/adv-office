      </div>
    </div>
    <div class="wrapper row1">
      <header id="header" class="hoc clear">
        <nav id="mainav" align="center">
          <ul class="clear">
            <li>
              <a href="/main" data-translatable>
                <%= __('homepage') %>
              </a>
            </li>
            <li>
              <a href="/aboutus">
                <%= __('aboutus') %>
              </a>
            </li>
            <li>
              <a href="/field">
                <%= __('fieldof') %>
              </a>
            </li>
            <li>
              <a href="#">
                <%= __('reference') %>
              </a>
            </li>
            <li>
              <a href="/contact">
                <%= __('contact') %>
              </a>
            </li>
          </ul>
        </nav>
      </header>
    </div>

    <!-- Top Background Image Wrapper -->
    <div class="bgded overlay ftr-background" style="height: 400px;">
      <img style="height: 100%; object-fit:cover;" src="images/demo/backgrounds/reference.jpg" alt="aboutus">
    </div>

    <div class="center" align="center">
      <img style="width: 60px; margin-top: 50px; margin-bottom: 10px;" src="/images/vaga-logo.png" alt="copyright-logo">
      <h2 style="width: 181px; margin: auto;" id="contact-title"><%= __('referencetitle') %></h2>
    </div>

    <div class="wrapper container">
      <div class="hoc clear">
        <p class="field-about"> <%= __('reference1') %></p>
      </div>
    </div>

    

    <div class="wrapper row5">
      <div id="copyright" class="hoc clear">
        <p class="fl_left">
          <img style="width: 250px;" src="/images/white-logo.png" alt="copyright-logo">
        </p>
        <p class="fl_right allrights">
          <a>Law office Belgrade. All rights reserved.</a>
        </p>
      </div>
    </div>
    <!-- JAVASCRIPTS -->
    <script src="layout/scripts/jquery.min.js"></script>
